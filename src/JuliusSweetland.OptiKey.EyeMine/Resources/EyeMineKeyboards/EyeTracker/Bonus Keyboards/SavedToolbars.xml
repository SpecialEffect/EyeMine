<Keyboard>
  <Name>Saved\nToolbars</Name>
  <BorderThickness>0,0,0,0</BorderThickness>
  <Grid>
    <Rows>5</Rows>
    <Cols>7</Cols>
  </Grid>
  <Content>
    <DynamicKey AutoScaleToOneKeyWidth="false"      
     Row="0" Col="0" Width="7">
      <Label>You can save and load your hotbars only when the inventory is closed</Label>
    </DynamicKey>
    <DynamicKey Row="1" Col="0" Height="2">
      <Label>SAVE</Label>
      <KeyToggle>c</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="1" Col="1" Height="2">
      <Label>1</Label>
      <Text>1</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="2" Height="2">
      <Label>2</Label>
      <Text>2</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="3" Height="2">
      <Label>3</Label>
      <Text>3</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="4" Height="2">
      <Label>4</Label>
      <Text>4</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>    
    <DynamicKey Row="1" Col="5" Height="2" SharedSizeGroup="Inventory">
      <Label>Inventory</Label>
      <Text>e</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="6" Height="2" SharedSizeGroup="SavedToolbars">
      <Label>Toolbars\nTab</Label>
      <Action>NumPad4</Action>
    </DynamicKey>
    <DynamicKey Row="3" Col="0" Height="2">
      <Label>LOAD</Label>
      <KeyToggle>x</KeyToggle>      
    </DynamicKey>
    <DynamicKey Row="3" Col="1" Height="2">
      <Label>5</Label>
      <Text>5</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="3" Col="2" Height="2">
      <Label>6</Label>
      <Text>6</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="3" Col="3" Height="2">
      <Label>7</Label>
      <Text>7</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="3" Col="4" Height="2">
      <Label>8</Label>
      <Text>8</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    <DynamicKey Row="3" Col="5" Height="2">
      <Label>9</Label>
      <Text>9</Text>
      <Wait>100</Wait>
      <KeyUp>c</KeyUp>
      <KeyUp>x</KeyUp>
    </DynamicKey>
    

    <DynamicKey Row="3" Col="6" Height="2">
      <Symbol>BackIcon</Symbol>
      <Action>BackFromKeyboard</Action>
    </DynamicKey>
  </Content>
</Keyboard>