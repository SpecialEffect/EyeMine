
<Keyboard>
   <Height>32</Height>
   <Name>Minecraft 10x3 inventory</Name>
   <SymbolMargin>20</SymbolMargin>
   <Grid>
      <Rows>2</Rows>
      <Cols>10</Cols>
   </Grid>
   <HideFromKeyboardMenu>True</HideFromKeyboardMenu>
   <InitialKeyStates>
     <KeyUp>MouseMagneticCursor</KeyUp>
     <KeyLockedDown>MouseMagnifier</KeyLockedDown>   
   </InitialKeyStates>
	<Keys>
		
		<ActionKey>
			<Label>Esc</Label>
			<Symbol>MinecraftEscButton</Symbol>
			<Action>Escape</Action>
			<Row>0</Row>
			<Col>9</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<TextKey>
			<Label>Open\nInventory</Label>
			<Text>e</Text>
			<Row>0</Row>
			<Col>0</Col>
			<Height>1</Height>
			<Width>1</Width>
		</TextKey>
		<ActionKey>
			<Label>Building</Label>
			<Symbol>MinecraftTabBlocks</Symbol>
			<Action>NumPad0</Action>
			<Row>0</Row>
			<Col>1</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Decoration</Label>
			<Symbol>MinecraftTabFlower</Symbol>
			<Action>NumPad1</Action>
			<Row>0</Row>
			<Col>2</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Redstone</Label>
			<Symbol>MinecraftTabGem</Symbol>
			<Action>NumPad2</Action>
			<Row>0</Row>
			<Col>3</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Transport</Label>
			<Symbol>MinecraftTabLadder</Symbol>
			<Action>NumPad3</Action>
			<Row>0</Row>
			<Col>4</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Misc</Label>
			<Symbol>MinecraftTabBucket</Symbol>
			<Action>NumPad4</Action>
			<Row>0</Row>
			<Col>5</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>

		<ActionKey>
			<Label>Search</Label>
            <Symbol>MinecraftTabSearch</Symbol>            
			<Action>ArrowDown</Action>
			<Row>0</Row>
			<Col>7</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ChangeKeyboardKey>
			<Label>ABC</Label>
			<Symbol>AlphaIcon</Symbol>
            <DestinationKeyboard>3_ChatKeyboard</DestinationKeyboard>
            <ReturnToThisKeyboard>True</ReturnToThisKeyboard>
			<Row>1</Row>
			<Col>7</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ChangeKeyboardKey>
		<TextKey>
			<Label>Throw\nitem</Label>
			<Text>q</Text>
			<Row>1</Row>
			<Col>0</Col>
			<Height>1</Height>
			<Width>1</Width>
		</TextKey>
		<ActionKey>
			<Label>Foodstuffs</Label>
			<Symbol>MinecraftTabApple</Symbol>
			<Action>NumPad5</Action>
			<Row>1</Row>
			<Col>1</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Tools</Label>
			<Symbol>MinecraftTabAxe</Symbol>
			<Action>NumPad6</Action>
			<Row>1</Row>
			<Col>2</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Combat</Label>
			<Symbol>MinecraftTabSword</Symbol>
			<Action>NumPad7</Action>
			<Row>1</Row>
			<Col>3</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Brewing</Label>
			<Symbol>MinecraftTabPotion</Symbol>
			<Action>NumPad8</Action>
			<Row>1</Row>
			<Col>4</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Materials</Label>
			<Symbol>MinecraftTabStick</Symbol>
			<Action>NumPad9</Action>
			<Row>1</Row>
			<Col>5</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
        
		<ActionKey>
			<Label>Left\nclick</Label>
			<Symbol>MouseLeftClickWithMouseIcon</Symbol>
			<Action>MouseMoveAndLeftClick</Action>
			<Row>0</Row>
			<Col>8</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
        <ActionKey>
			<Label>Discard</Label>			
			<Action>Home</Action>
			<Row>1</Row>
			<Col>8</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
        
        <ActionKey>
			<Label>Scroll\nUp</Label>
			<Symbol>MouseScrollToTopIcon</Symbol>
			<Action>F8</Action>
			<Row>0</Row>
			<Col>6</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Scroll\nDown</Label>
			<Symbol>MouseScrollToBottomIcon</Symbol>
			<Action>F9</Action>
			<Row>1</Row>
			<Col>6</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
		<ActionKey>
			<Label>Back</Label>
			<Symbol>BackIcon</Symbol>
			<Action>BackFromKeyboard</Action>
			<Row>1</Row>
			<Col>9</Col>
			<Height>1</Height>
			<Width>1</Width>
		</ActionKey>
	</Keys>
</Keyboard>